   "source": [
    "number = int(input(\"Enter a number to fin out all its divisors: \"))\n",
    "a = []\n",
    "\n",
    "for i in range(1, number+1) :\n",
    "    if number % i == 0 :\n",
    "        a.append(i)\n",
    "        i += 1\n",
    "print(a)"
